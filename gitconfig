[user]
	email = satgo1546@yeah.net
	name = Frog Chen
[push]
	default = simple
[credential]
	helper = cache --timeout=14400
[color]
	ui = true
[column]
	ui = auto
[alias]
	co = checkout
	com = checkout master
	cos = checkout satgo1546-dev
	ci = commit
	st = status -sb
	dup = clone --depth=1 --no-single-branch --recursive --shallow-submodules
	graph = log --all --graph --abbrev-commit --pretty=tformat:'%>|(15)%Cred%h %C(yellow)%d%Creset %<|(66,trunc)%s %Cgreen%>(7)%cr %Cblue%an'
	undo-commit = reset HEAD~
	undo-merge = reset --hard ORIG_HEAD
	unstage = reset HEAD --
	last = log -1 HEAD
	br = branch
[core]
	editor = gvim -f -S ~/miscellaneous/scripts/popup-edit.vim
	excludesfile = /home/satgo1546/.gitignore_global
	eol = lf
	safecrlf = true
[merge]
	renormalize = true
[help]
	autoCorrect = 5
