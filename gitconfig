[push]
	default = simple
[credential]
	helper = store
[color]
	ui = true
[column]
	ui = auto
[alias]
	co = checkout
	com = checkout master
	cos = checkout satgo1546-dev
	cog = checkout gh-pages
	cob = checkout bobcao3-dev
	mg = merge
	mgm = merge master
	mgs = merge satgo1546-dev
	mgg = merge gh-pages
	mgb = merge bobcao3-dev
	br = branch
	ci = commit
	st = status -sb
	dup = clone --depth=1 --no-single-branch --recursive --shallow-submodules
	graph = log --all --graph --abbrev-commit --pretty=tformat:'%>|(15)%Cred%h %C(auto)%d%Creset %<|(66,trunc)%s %Cgreen%>(7)%ar %Cblue%an'
	undo-commit = reset HEAD~
	undo-merge = reset --hard ORIG_HEAD
	unstage = reset HEAD --
	last = log -1 HEAD
[core]
	editor = gvim -f
	excludesfile = D:\\Miscellaneous\\gitignore
	eol = lf
	safecrlf = warn
	autocrlf = input
	fscache = true
[merge]
	renormalize = true
[help]
	autoCorrect = 5
[gpg]
	program = gpg2
