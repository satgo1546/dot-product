	signingkey = 2AE5A66FF1D92739
[push]
	default = simple
	autoSetupRemote = true
[pull]
	rebase = true
[credential]
	helper = store
[color]
	ui = true
[column]
	ui = auto
[alias]
	co = checkout
	com = checkout master
	cos = checkout satgo1546-dev
	cog = checkout gh-pages
	cob = checkout bobcao3-dev
	mg = merge --allow-unrelated-histories
	mgm = merge master
	mgs = merge satgo1546-dev
	mgg = merge gh-pages
	mgb = merge bobcao3-dev
	br = branch
	ci = commit
	st = status -sb
	dup = clone --depth=1 --no-single-branch --recursive --shallow-submodules
	graph = log --all --graph --abbrev-commit --pretty=tformat:'%>|(15)%Cred%h %C(auto)%d%Creset %<|(66,trunc)%s %Cgreen%>(7)%ar %Cblue%an'
	undo-commit = reset HEAD~
	undo-merge = reset --hard ORIG_HEAD
	unstage = reset HEAD --
	last = log -1 HEAD
[core]
	editor = vim
	eol = lf
	safecrlf = warn
	autocrlf = input
	fscache = true
	excludesfile = ~/.gitignore
[rerere]
	enabled = true
	autoupdate = true
[merge]
	renormalize = true
[rebase]
	autoSquash = true
	autoStash = true
	updateRefs = true
[help]
	autoCorrect = prompt
[gpg]
	program = gpg2
[http]
	proxy = http://127.0.0.1:7890
[https]
	proxy = http://127.0.0.1:7890
[diff]
	algorithm = histogram
	colorMoved = plain
	mnemonicPrefix = true
	renames = true
	tool = difftastic
[difftool]
	prompt = false
[difftool "difftastic"]
	cmd = difft "$LOCAL" "$REMOTE"
[pager]
	difftool = true
[branch]
	sort = -committerdate
[tag]
	sort = version:refname
[init]
	defaultBranch = main
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
